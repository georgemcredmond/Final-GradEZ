<!DOCTYPE html>
<html>
  <head>
    <title>Lms</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% if current_teacher %>
    <h1>You made it! <%= current_teacher.email if current_teacher %></h1>
    <h1>Can you see this</h1>
    <%= link_to "Sign Out", destroy_teacher_session_path, method: :delete %>
  <% else %>
    <%= link_to "Sign In", new_teacher_session_path %>

  <% end %>
    <%= yield %>
  </body>
</html>
