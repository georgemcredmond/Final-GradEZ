<!DOCTYPE html>
<html>
  <head>
    <title>Lms</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="wrapper">
      <div id="navbar">
        <ul class="topnav1">
  <% if current_teacher %>
    <li>Welcome <%= current_teacher.email if current_teacher %></li>
    <li><%= link_to "Sign Out", destroy_teacher_session_path, method: :delete %>
  <% else %>
    <%= link_to "Sign In", new_teacher_session_path %></li>
  <% end %> 
        </ul>

      </div>
    </div>
    <%= yield %>
  </body>
</html>
