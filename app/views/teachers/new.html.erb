<%= @Message %>

<h3>
<%= form_for Teacher.new do |f| %>
	<%= f.label :first_name %>:
	<%= f.text_field :first_name %> <br><br>
	<%= f.label :last_name %>:
	<%= f.text_field :last_name %> <br><br>
	<%= f.label :email %>:
	<%= f.text_field :email %> <br><br>
	<%= f.label :encrypted_password %>:
	<%= f.text_field :encrypted_password %><br><br>
	<%= f.submit %>
</h3>
<% end %>